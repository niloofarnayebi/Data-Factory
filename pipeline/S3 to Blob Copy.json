{
	"name": "S3 to Blob Copy",
	"properties": {
		"activities": [
			{
				"name": "S3 to Azure blob",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "FileSystemSource",
						"recursive": true
					},
					"sink": {
						"type": "BlobSink"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "datasetS3",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureBlob1",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Notebook1",
				"type": "DatabricksNotebook",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"notebookPath": "/dataengineering with databricks/1 Datafactory ETL",
					"baseParameters": {
						"input": {
							"value": "@pipeline().parameters.inputPath",
							"type": "Expression"
						},
						"output": {
							"value": "@pipeline().parameters.outputPath",
							"type": "Expression"
						},
						"fileName": {
							"value": "@pipeline().parameters.fileName",
							"type": "Expression"
						},
						"piplineRunId": {
							"value": "@pipeline().RunId",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks1",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"inputPath": {
				"type": "string",
				"defaultValue": "/"
			},
			"outputPath": {
				"type": "string",
				"defaultValue": "/"
			},
			"fileName": {
				"type": "string",
				"defaultValue": "Product.csv"
			}
		}
	}
}